{
  "manifest_version": 2,
  "name": "Open Tabs",
  "version": "1.0",
  "description": "展示当前浏览器打开的窗口列表，包含标题和网址，点击可以跳转到对应的标签页。",
  "permissions": ["tabs", "activeTab"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
